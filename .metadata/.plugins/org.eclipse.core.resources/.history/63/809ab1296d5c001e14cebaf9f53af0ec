package point;

public class Point {
	char nom;
	int abs;
	int ord;


public void affiche ()
	{
		System.out.println(nom+"("+abs+","+ord+")");
	}
public void deplacer(int a,int b)
	{
		abs=abs+a;
		ord=ord+b;
	}
public void rest()
{
	abs=0;
	ord=0;
	
	
}

public Point(char c, int i, int j)
{}
public Point(Point p)
{

}

}


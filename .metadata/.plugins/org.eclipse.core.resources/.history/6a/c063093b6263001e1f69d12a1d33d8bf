package tp3;

public class Personne {

	
	private String nom;
	private String prenom;
	private String adress;
	Personne(String nom, String prenom, String adress)
	{
		this.nom=nom;
		this.prenom=prenom;
		this.adress=adress;
	}

	
 public String toString() {
	 return "nom : "+this.nom+"prenom :"+this.prenom+"adresse : "+this.adress;
 }

}


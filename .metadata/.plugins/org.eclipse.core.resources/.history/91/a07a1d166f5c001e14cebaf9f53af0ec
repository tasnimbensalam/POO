package point;

public class TestPoint {
public static void main(String[] args) {
	Point p1=new Point();
	p1.affiche();
	p1.abs=2;
	p1.ord=-1;
	p1.nom='a';
	p1.affiche();
	p1.deplacer(1, -2);
	p1.affiche();
	p1.rest();
	p1.affiche();
	Point p2=new Point('A',3,5);
	Point p3=new Point(p1);

	p2.affiche();
	p3.affiche();

}}

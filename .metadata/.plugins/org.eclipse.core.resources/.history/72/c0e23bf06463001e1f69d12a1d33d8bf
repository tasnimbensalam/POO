package tp3;


class Compte {
		private int num;
		Personne titulaire;
		private double sold;
		private double decouvertMax;
		private double debitMax;
		private static int count;
			
 

 Compte(Personne p)
{
	this.titulaire=p;
	this.debitMax=1000;
	this.decouvertMax=100;
}

Compte(Personne p,int num,double solde,double debitMax,double decouvertMax)
{
	this.num=num;
	this.titulaire=p;
	this.sold=solde;
	this.debitMax=debitMax;
	this.decouvertMax=decouvertMax;
	
}

public String toString() {
	 return "num : "+this.num+"titulaire :"+this.titulaire+"solde : "+this.sold+"debitMax : "+this.debitMax+"decouvertMax : "+this.decouvertMax;
}
 
 
}


